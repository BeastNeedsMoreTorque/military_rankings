    "    plt.xticks(df_battles.index, df_battles['Battle'], rotation = 'vertical')\n",
    "\n",
    "    plt.title(commander + ' ({} - {} - {})'.format(wins, draws, losses))\n",
    "    plt.tight_layout()\n",
    "    \n",
    "    plt.savefig('{}_warg.png'.format(commander))"
